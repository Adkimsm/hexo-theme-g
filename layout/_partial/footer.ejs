<div id="footer">
    <div id="footer-content" class="clear">
        <div id="footer-content-left">
            <p id="footer-time">Â© <%= config.title %> | <%- __('btext') %><span id="site_runtime"></span></p>
            <nav id="nav-2">
                <% for (var i in theme.pages){ %>
                <a href="<%= theme.pages[i].link %>"><%= i %></a>
                <% } %>
            </nav>
        </div>


    </div>

    <%- js('JS/X.js') %>
    <%- js('JS/prism.js') %>
    <script src="<%- url_for('/JS/toastr.min.js') %>"></script>

    <script src="<%- url_for('/JS/SmoothScroll.min.js') %>"></script>
    <script>
        window.SmoothScroll({
            stepSize: 40,
            animationTime: 400
        })
    </script>

</div>



<div id="m_toc" onclick="toc_toggle();">
    <span><a><i class="i m_toc"></i></a></span>
</div>

<div id="m_top" onclick="gototop();">
    <span><a><i class="i gototop"></i></a></span>
</div>

<div id="m_menu" onclick="sideMenu_toggle();">
    <span><a><i class="i m_menu"></i></a></span>
</div>

<div id="m_night" onclick="switchNightMode();">
    <span><a><i class="i m_night"></i></a></span>
</div>

<div id="sliderbar" class="move_left">

    <div class="sliderbar-content-menu">
        <div class="Sliderbar-content clear">
            <div class="Sliderbar-content-switch clear">
                <h2>PAGES</h2>
                <a onclick="show_slide_content(2);"><i class="i down"></i></a>
            </div>
            <div id="Sliderbar-content-2" class="clear">
                <% for (var o in theme.pages){ %>
                <a href="<%= theme.pages[o].link %>" onclick="sideMenu_toggle()"><%= o %></a>
                <% } %>
            </div>
        </div>
    </div>
</div>


</div>
<div id="sliderbar-cover" onclick="sideMenu_toggle();"></div>
<div id="sliderbar-toc-cover" onclick="toc_toggle();"></div>

<div id="sliderbar-toc" class="is-position-fixed move_right">
    <div class="toc">
        <ol class="toc-list">
            <%- toc(page.content,{list_number:false}) || render('<ol class="toc"><li class="toc-item toc-level-3"><a class="toc-link" style="cursor: normal;"><span class="toc-text">Nothing.</span></a></li></ol>', 'ejs'); %>
        </ol>
    </div>
</div>

<script>
    collapse_toggle();
    show_site_runtime("<%= theme.buildtime %>");
</script>
</body>

</html>